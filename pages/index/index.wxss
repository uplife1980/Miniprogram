.book {
  position: relative;
  margin: 0% 0% 0% 0%;
  width: 80%;
  height: 500rpx;
  box-shadow: 0 3px 15px rgba(0, 0, 0, 0.5);
}
.book .page {
  position: absolute;
  top: 0;
  bottom: 0;
  width: 50%;
  background:#fff;
}
.book #first {
  left: 0;
  box-shadow: inset -30px 0 60px -30px rgba(0, 0, 0, 0.7);
  background:url('data:image/png;base64,');
  background-size: 100% 100%;
  

}
.book #first:before {
  content: "";
  position: absolute;
  z-index: -1;
  right: 0;
  bottom: 0%;
  height: 5%;
  width: 60%;
  border-radius: 100%;
  box-shadow: 0 10px 30px rgba(0, 0, 0, 0.5);
  
}
.book #second {
  right: 0;
  box-shadow: inset 30px 0 50px -30px rgba(0, 0, 0, 0.5);
   border-radius: 3px; 
   background:rgba(255, 209, 80, 1);
}
.book #second:before {
  content: "";
  position: absolute;
  z-index: -1;
  left: 0;
  bottom: 0%;
  height: 5%;
  width: 60%;
  border-radius: 100%;
  box-shadow: 0 10px 30px rgba(0, 0, 0, 0.4);
}



